<TheGame>
   <game>
        <!-- Size of a block, default (int)15 -->
        <block_size>15</block_size>
        <!-- Size of map, default (int)700 -->
        <map_size>700</map_size>
        <!-- FPS, default (int)60 -->
        <fps>60</fps>
        <!-- Max speed, default (float)map_size/70 -->
        <block_max_speed>10.0</block_max_speed>
        <!-- Block acceleration, default (float)(block_max_speed/70)*2 -->
        <block_acceleration>0.2857142857</block_acceleration>
        <!-- Bullet size, default (int)2 -->
        <bullet_size>2</bullet_size>
        <!-- Gun size, default (float)1.9 -->
        <gun_size>1.9</gun_size>
        <!-- Footer size, default (int)20 -->
        <footer_size>20</footer_size>
        <!-- Bullet speed, default (float)1.5*block_max_speed  -->
        <bullet_speed>15.0</bullet_speed>
        <!-- Thickness, default 2 -->
        <thickness>2</thickness>
        <!-- Respawn time in seconds, default (float)2 -->
        <respawn_time>2</respawn_time>
        <!-- Gun resolution in radians, default (float)block_size/map_size*2 -->
        <gun_resolution>0.0428571428</gun_resolution> =
        <!-- Max ammo, default (int)200 -->
        <max_ammo>200</max_ammo>
        <!-- Player health, default (int)200 -->
        <hp>200</hp>
    </game>
</TheGame>